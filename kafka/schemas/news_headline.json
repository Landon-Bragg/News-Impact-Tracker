{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "news_headline",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "source": {
      "type": "string"
    },
    "headline": {
      "type": "string"
    },
    "tickers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id",
    "timestamp",
    "headline"
  ]
}